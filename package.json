  "devDependencies": {
    "@cliqz/adblocker": "^1.26.12",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": ">=6.0.0",
    "@typescript-eslint/parser": ">=6.0.0",
    "ava": "^6.0.1",
    "eslint": ">=8.0.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^1.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
